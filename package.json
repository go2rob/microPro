{
  "name": "law2cf",
  "version": "1.0.0",
  "description": "look at where we came from",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "watch": {
    "test": {
      "patterns": [
        "src",
        "test"
      ],
      "extensions": "js,jsx",
      "quiet": true,
      "legacyWatch": true,
      "delay": 2500
    }
  },
  "scripts": {
    "start": "http-server -p 8888 -c-1",
    "watch": "npm-watch",
    "test": "nodemon --watch . --exec 'mocha --compilers js:babel-register --no-deprecation --require testHelper.js \"./test/*@(.js)\" || true'",
    "debug": "node inspect node_modules/mocha/bin/_mocha  --compilers js:babel-register --no-deprecation --require testHelper.js \"./test/*@(.js)\" || true'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/go2rob/microPro.git"
  },
  "keywords": [
    "souvenir"
  ],
  "author": "robincEPtion",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/go2rob/microPro/issues"
  },
  "homepage": "https://github.com/go2rob/microPro#readme",
  "dependencies": {
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "bootstrap": "^4.0.0-beta.2",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "node-inspect": "^1.11.2",
    "pg": "^7.3.0",
    "requirejs": "^2.3.5",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "npm-watch": "^0.3.0",
    "mocha": "^4.0.1",
    "chai": "^4.1.2",
    "jsdom": "11.3.0",
    "jsdom-global": "3.0.2"
  }
}
